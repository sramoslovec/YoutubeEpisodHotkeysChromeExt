{
    "name": "Youtube Episode Hotkeys",
    "version": "1.1",
    "description": "Q - prev ep; E - next ep",
    "permissions": ["storage"],
    "content_scripts": [ {
        "matches": ["https://www.youtube.com/*"],
        "js": ["files/content_scripts/script.js"]
    } ],
    "icons": {
        "128": "files/icons/icon128.png"
    },
    "browser_action": {
        "default_popup": "files/popup.html"
    },
    "manifest_version": 2
}
